    PremiumOnly = false
})

TradeScamTab:AddButton({
    Name = "Force Confirm Trade âœ…",
    Callback = function()
        print("Starting trade confirmation...")
        OrionLib:MakeNotification({
            Name = "Confirming Trade... â³",
